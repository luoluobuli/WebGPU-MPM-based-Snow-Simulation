	},
	"devDependencies": {
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/three": "^0.181.0",
		"@vitest/browser": "^3.2.4",
		"@webgpu/types": "^0.1.66",
		"playwright": "^1.56.1",
		"sass": "^1.93.3",
		"svelte": "^5.43.5",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
